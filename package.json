{
  "name": "denali-icons",
  "description": "",
  "version": "0.0.0-developmnet",
  "scripts": {
    "build": "npm-run-all --sequential json sprite font",
    "font": "rm -rf dist/font && mkdir dist/font && fantasticon -o dist/font -n denali-icons-font -t woff2 -g css --selector .d-icon -p d svg",
    "json": "iconit --name=denali-icons --json",
    "sprite": "iconit --name=denali-icons-sprite --sprite",
    "icons": "rm -rf dist/icons && mkdir dist/icons && cp -R svg dist"
  },
  "repository": {
    "type": "git",
    "url": "https://git.vzbuilders.com/denali-system-language/denali-icons.git"
  },
  "keywords": [
    "icons",
    "sprite",
    "font"
  ],
  "author": "Chas Turansky <cturansky@verizonmedia.com> (http://turansky.net/)",
  "license": "MIT",
  "bugs": {
    "url": "https://git.vzbuilders.com/denali-system-language/denali-icons"
  },
  "homepage": "https://git.vzbuilders.com/denali-system-language/",
  "devDependencies": {
    "fantasticon": "^1.1.1",
    "iconit": "^1.0.0",
    "npm-run-all": "^4.1.5"
  }
}
